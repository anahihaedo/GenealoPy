BEGIN TRANSACTION;DROP TABLE IF EXISTS `personne`;CREATE TABLE IF NOT EXISTS `personne` (	`personne_id`	integer NOT NULL PRIMARY KEY AUTOINCREMENT,	`personne_nom`	TINYTEXT NOT NULL,	`personne_prenom`	TINYTEXT NOT NULL);DROP TABLE IF EXISTS `place`;CREATE TABLE IF NOT EXISTS `place` (	`place_id`	integer NOT NULL PRIMARY KEY AUTOINCREMENT,	`place_ville`	TINYTEXT NOT NULL,	`place_pays`	TINYTEXT NOT NULL);DROP TABLE IF EXISTS `relation_personnes`;CREATE TABLE IF NOT EXISTS `Relation_personnes` (	`relation_id`	integer NOT NULL PRIMARY KEY AUTOINCREMENT,	`relation_nom`	TINYTEXT NOT NULL);DROP TABLE IF EXISTS `relation_ville`;CREATE TABLE IF NOT EXISTS `relation_ville` (	`relation_id`	integer NOT NULL PRIMARY KEY AUTOINCREMENT,	`naissance_ville`	TINYTEXT NOT NULL,	`naissance_pays`	TINYTEXT NOT NULL,	`mariage_ville`	TINYTEXT NOT NULL,	`mariage_pays`	TINYTEXT NOT NULL,	`deces_ville`	TINYTEXT NOT NULL,	`deces_pays`	TINYTEXT NOT NULL);DROP TABLE IF EXISTS `evenement`;CREATE TABLE IF NOT EXISTS `evenement` (	`evenement_id`	integer NOT NULL PRIMARY KEY AUTOINCREMENT,	`evenement_nom`	TINYTEXT NOT NULL);DROP TABLE IF EXISTS `date`;CREATE TABLE IF NOT EXISTS `date` (	`date_id`	integer NOT NULL PRIMARY KEY AUTOINCREMENT,	`dates`	TINYTEXT NOT NULL,	`personne_evenement`	TINYTEXT NOT NULL);END TRANSACTION;BEGIN TRANSACTION;INSERT INTO `personne` (`personne_id`, `personne_nom`, `personne_prenom`) VALUES (1, 'Poggi', 'Tomas');INSERT INTO `personne` (`personne_id`, `personne_nom`, `personne_prenom`) VALUES (2, 'Poggi', 'Blanca'); INSERT INTO `personne` (`personne_id`, `personne_nom`, `personne_prenom`) VALUES (3, 'Poggi', 'Santiago'); INSERT INTO `personne` (`personne_id`, `personne_nom`, `personne_prenom`) VALUES (4, 'Rocca', 'Teresa'); INSERT INTO `personne` (`personne_id`, `personne_nom`, `personne_prenom`) VALUES (5, 'Poggi', 'Cristina'); INSERT INTO `personne` (`personne_id`, `personne_nom`, `personne_prenom`) VALUES (6, 'Gonza